<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx/8.0.66" xmlns:fx="http://javafx.com/fxml/1" fx:controller="headhunt.app.dialogs.settings.SettingsPresenter">
   <children>
      <VBox spacing="10.0">
         <children>
            <Label text="Vimeo API scraping:" />
            <Separator prefWidth="200.0" />
            <GridPane hgap="10.0">
              <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="125.0" minWidth="10.0" prefWidth="117.0" />
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="220.0" minWidth="10.0" prefWidth="219.0" />
                  <ColumnConstraints halignment="RIGHT" hgrow="SOMETIMES" maxWidth="128.0" minWidth="10.0" prefWidth="33.0" />
              </columnConstraints>
              <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
              </rowConstraints>
               <children>
                  <Label text="On fail wait:" />
                  <Label text="min" GridPane.columnIndex="2" />
                  <TextField fx:id="vimeoOnFailWaitLabel" GridPane.columnIndex="1" />
                  <Label text="On success wait:" GridPane.rowIndex="1" />
                  <TextField fx:id="vimeoOnSuccessWaitLabel" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                  <Label text="min" GridPane.columnIndex="2" GridPane.rowIndex="1" />
               </children>
               <padding>
                  <Insets left="10.0" right="10.0" />
               </padding>
            </GridPane>
            <HBox alignment="TOP_RIGHT" spacing="5.0">
               <children>
                  <Button fx:id="okButton" mnemonicParsing="false" onAction="#ok" text="Ok" />
                  <Button fx:id="cancelButton" mnemonicParsing="false" onAction="#cancel" text="Cancel" />
               </children>
            </HBox>
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding></VBox>
   </children>
</AnchorPane>
