<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx/8.0.66" xmlns:fx="http://javafx.com/fxml/1" fx:controller="headhunt.app.dialogs.scraper.create.CreatePresenter">
   <children>
      <VBox spacing="10.0">
         <children>
            <GridPane hgap="5.0" vgap="5.0">
              <columnConstraints>
                <ColumnConstraints halignment="RIGHT" hgrow="SOMETIMES" maxWidth="188.0" minWidth="10.0" prefWidth="59.0" />
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="345.0" minWidth="10.0" prefWidth="331.0" />
              </columnConstraints>
              <rowConstraints>
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
              </rowConstraints>
               <children>
                  <Label text="Name:" GridPane.rowIndex="1" />
                  <Label text="Token:" GridPane.rowIndex="2" />
                  <TextField fx:id="nameInput" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                  <TextField fx:id="tokenInput" GridPane.columnIndex="1" GridPane.rowIndex="2" />
                  <Label text="Query:" GridPane.rowIndex="3" />
                  <TextField fx:id="queryInput" GridPane.columnIndex="1" GridPane.rowIndex="3" />
                  <Label text="Scraper:" />
                  <MenuButton fx:id="classMenuButton" mnemonicParsing="false" prefHeight="26.0" prefWidth="165.0" text="Click me..." GridPane.columnIndex="1" />
               </children>
               <padding>
                  <Insets left="10.0" />
               </padding>
            </GridPane>
            <HBox alignment="TOP_RIGHT" spacing="5.0">
               <children>
                  <Button fx:id="cancelButton" mnemonicParsing="false" onAction="#cancel" text="Cancel" />
                  <Button fx:id="createButton" mnemonicParsing="false" onAction="#create" text="Save">
                     <HBox.margin>
                        <Insets />
                     </HBox.margin>
                  </Button>
               </children>
            </HBox>
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </VBox>
   </children>
</AnchorPane>
